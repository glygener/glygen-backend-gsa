        "emlist":[
          {
            "emid":"seq_format", "emtype":"select", "datatype":"string",
            "label":"Sequence Format", "class":"submissionsform",
            "style":{"width":"25%"},
            "value":{
              "selected":"GlycoCT",
              "optlist":[
                {"value":"GlycoCT", "label":"GlycoCT"},
                {"value":"WURCS", "label":"WURCS"}
              ]
            }
          },
          {
            "emid":"seq", "emtype":"textarea", "datatype":"string",
            "label":"Glycan Sequence", "class":"submissionsform",
            "style":{"width":"100%", "margin":"10px 0px 0px 0px"},
            "value":""
          },
          {
            "emid":"cancelbtn", "emtype":"button", "datatype":"string",
            "onclick":"handleCancel", "class":"btn btn-outline-secondary",
            "value":"Cancel",
            "style":{"display":"block", "float":"left", "margin":"10px 10px 0px 0px"}
          },
          {
            "emid":"submitbtn", "emtype":"button", "datatype":"string",
            "class":"btn btn-outline-secondary",
            "value":"Next",
            "style":{"display":"block", "float":"left", "margin":"10px 10px 0px 0px"}
          }
        ]
      }
    ]
  },
