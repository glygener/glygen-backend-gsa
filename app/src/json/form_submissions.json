{
  "step_one":{
    "title":"Select molecule type and source",
    "titlestyle":{"width":"100%", "margin":"0px 0px 15px 0px", "fontWeight":"bold"},
    "class":"submissionsform",
    "groups":[
      {
        "groupid":"one",
        "style":{"width":"40%","margin":"0px 0px 0px 0px"},
        "emlist":[
          {
            "emid":"molecule", "emtype":"select", "datatype":"string", 
            "label":"Molecule Type", "class":"submissionsform", 
            "value":{
              "selected":"GlycoProtein",
              "optlist":[
                {"value":"Glycan", "label":"Glycan"},
                {"value":"GlycoProtein", "label":"GlycoProtein"},
                {"value":"GlycoPeptide", "label":"GlycoPeptide"},
                {"value":"GlycoLipid", "label":"GlycoLipid"},
                {"value":"GPI", "label":"GPI"},
                {"value":"Other Glycoconjugate", "label":"Other Glycoconjugate"}
              ]
            }
          },
          {
            "emid":"source", "emtype":"select", "datatype":"string",
            "label":"Source", "class":"submissionsform",
            "value":{
              "selected":"Biological",
              "optlist":[
                {"value":"Biological", "label":"Biological"},
                {"value":"Recombinant expressed", "label":"Recombinant expressed"},
                {"value":"Synthetic", "label":"Synthetic"},
                {"value":"Computational", "label":"Computational"}
              ]
            }
          },
          {
            "emid":"cancelbtn", "emtype":"button", "datatype":"string",
            "onclick":"handleCancel", "class":"btn btn-outline-secondary",
            "value":"Cancel", 
            "style":{"display":"block", "float":"left", "margin":"10px 10px 0px 0px"}
          },
          {
            "emid":"submitbtn", "emtype":"button", "datatype":"string",
            "class":"btn btn-outline-secondary",
            "value":"Next",
            "style":{"display":"block", "float":"left", "margin":"10px 10px 0px 0px"}
          }
        ]
      }
    ]
  },
  "step_two_glycan":{
